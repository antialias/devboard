{
  "name": "devcards-js-example",
  "version": "1.0.0",
  "description": "Example for devcards",
  "private": true,
  "kotatsu": {
    "config": "--public public/ --index public/index.html --devtool \\#cheap-source-map --babel --config webpack.config.js main.js"
  },
  "scripts": {
    "start": "kotatsu serve --progress $npm_package_kotatsu_config",
    "prestatic": "rm -r public/build",
    "static": "BABEL_ENV=production kotatsu build client -o public/build $npm_package_kotatsu_config",
    "lint": "eslint --max-warnings 0 ."
  },
  "author": "Glen Mailer <glen@stainlessed.co.uk>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "eslint": "^1.10.3",
    "eslint-plugin-no-class": "^0.1.0",
    "eslint-plugin-react": "^3.15.0",
    "kotatsu": "^0.9.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  }
}
